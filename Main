local folderMuscleKingFarming = AutoFarm:AddFolder("Muscle King Farming")
local folderLegendGymFarming = AutoFarm:AddFolder("Legend Gym Farming")

-- Muscle King functions
local function createMuscleKingFunction(name, position)
    return function(Value)
        getgenv()[name] = Value
        if Value then
            task.spawn(function()
                while getgenv()[name] do
                    local player = game.Players.LocalPlayer
                    local character = player.Character
                    if character and character:FindFirstChild("HumanoidRootPart") then
                        character.HumanoidRootPart.CFrame = CFrame.new(position)
                        task.wait(0.3)
                        local args = {
                            [1] = "useMachine",
                            [2] = workspace:WaitForChild("machinesFolder"):WaitForChild(name):WaitForChild("interactSeat")
                        }
                        game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("machineInteractRemote"):InvokeServer(unpack(args))
                    end
                    task.wait(0.05)
                end
            end)
        end
    end
end

folderMuscleKingFarming:AddToggle("Auto Muscle King Lift", createMuscleKingFunction("Muscle King Lift", Vector3.new(-8772.97266, 24.4272194, -5638.37402)))
folderMuscleKingFarming:AddToggle("Auto Muscle King Boulder", createMuscleKingFunction("King Boulder", Vector3.new(-8986.61621, 30.0510483, -5647.14404)))
folderMuscleKingFarming:AddToggle("Auto Muscle King Squat", createMuscleKingFunction("Muscle King Squat", Vector3.new(-8760.77051, 63.8689613, -6088.8042)))
folderMuscleKingFarming:AddToggle("Auto Muscle King Bench", createMuscleKingFunction("Muscle King Bench", Vector3.new(-8582.58887, 41.7540054, -6051.01318)))

